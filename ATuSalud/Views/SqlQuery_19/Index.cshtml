@using ATuSalud.ViewData
@model List<SqlQuery_19ViewData>
@{
    ViewData["Title"] = "Index";
}

<h3>Listado de los medicamentos que tienen efectos secundarios que pueden ser severos</h3>

@if (Model != null)
{
    <table class="table table-striped">
        <thead class="bg-info text-light">
            <tr>
                
                <th>Nombre medicamento</th>
                <th>efecto</th>
            </tr>
        </thead>
        @foreach (SqlQuery_19ViewData item in Model)
        {
            <tr>
                <td>@item.nombre_medicamento</td>
                <td>@item.nombre_efecto</td>
            </tr>
        }
    </table>
}

