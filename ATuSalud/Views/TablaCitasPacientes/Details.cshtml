@model ATuSalud.Models.TablaCitasPaciente

@{
    ViewData["Title"] = "Details";
}


<div>
    <h4>Detalles de la cita</h4>
    <hr />
    <dl class="row">
        <dt class="col-sm-2">
            Paciente:
        </dt>
        <dd class="col-sm-10">
            @Html.DisplayFor(model => model.TablaPaciente.Nombre)
        </dd>
        <dt class="col-sm-2">
            Profesional:
        </dt>
        <dd class="col-sm-10">
            @Html.DisplayFor(model => model.TablaProfesional.nombre)
        </dd>
        <dt class="col-sm-2">
            Fecha de atencion:
        </dt>
        <dd class="col-sm-10">
            @Html.DisplayFor(model => model.Fecha_atencion)
        </dd>
        <dt class="col-sm-2">
            Fecha de acabado:
        </dt>
        <dd class="col-sm-10">
            @Html.DisplayFor(model => model.Fecha_acabar)
        </dd>
        <dt class="col-sm-2">
            Observaciones generales:
        </dt>
        <dd class="col-sm-10">
            @Html.DisplayFor(model => model.Observaciones)
        </dd>
        <dt class="col-sm-2">
            Fecha de registro
        </dt>
        <dd class="col-sm-10">
            @Html.DisplayFor(model => model.Fecha_registro)
        </dd>
    </dl>
</div>
<div>
    <a class="btn btn-primary" asp-action="Edit" asp-route-id="@Model.Id">Editar</a> |
    <a asp-action="Index">Volver a la lista</a>
</div>
