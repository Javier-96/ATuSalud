@model ATuSalud.Models.TablaAntecedentes

@{
    ViewData["Title"] = "Details";
}


<div>
    <h4>Antecedentes</h4>
    <hr />
    <dl class="row">
        <dt class="col-sm-2">
            Paciente
        </dt>
        <dd class="col-sm-10">
            @Html.DisplayFor(model => model.Paciente.Nombre)
        </dd>
        <dt class="col-sm-2">
            @Html.DisplayNameFor(model => model.Familiar)
        </dt>
        <dd class="col-sm-10">
            @Html.DisplayFor(model => model.Familiar)
        </dd>
        <dt class="col-sm-2">
           Código CIAP
        </dt>
        <dd class="col-sm-10">
            @Html.DisplayFor(model => model.CodigoCiap.Enfermedad)
        </dd>
    </dl>
</div>
<div>
    <a asp-action="Edit" asp-route-id="@Model.Id">Editar</a> |
    <a asp-action="Index">Volver a la lista</a>
</div>
