@model ATuSalud.Models.TablaReservaEspacios

@{
    ViewData["Title"] = "Delete";
}



<h3>¿Quieres eliminar esta reserva?</h3>
<div>
   
    <hr />
    <dl class="row">
        <dt class = "col-sm-2">
            <label>ID Profesional</label>
        </dt>
        <dd class = "col-sm-10">
            @Html.DisplayFor(model => model.ProfesionalId)
        </dd>
        <dt class = "col-sm-2">
            @Html.DisplayNameFor(model => model.Espacio)
        </dt>
        <dd class = "col-sm-10">
            @Html.DisplayFor(model => model.Espacio)
        </dd>
        <dt class = "col-sm-2">
            <label>Fecha de Inicio</label>
        </dt>
        <dd class = "col-sm-10">
            @Html.DisplayFor(model => model.FechaInicio)
        </dd>
        <dt class = "col-sm-2">
            <label>Fecha de Final</label>
        </dt>
        <dd class = "col-sm-10">
            @Html.DisplayFor(model => model.FechaFin)
        </dd>
    </dl>
    
    <form asp-action="Delete">
        <input type="hidden" asp-for="Id" />
        <input type="submit" value="Eliminar" class="btn btn-danger" /> |
        <a asp-action="Index">Volver a la lista</a>
    </form>
</div>
