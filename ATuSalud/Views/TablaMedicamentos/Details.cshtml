@model ATuSalud.Models.TablaMedicamentos

@{
    ViewData["Title"] = "Details";
}


<div>
    <h4>Medicamento</h4>
    <hr />
    <dl class="row">
        <dt class = "col-sm-2">
            Medicamento
        </dt>
        <dd class = "col-sm-10">
            @Html.DisplayFor(model => model.Nombre_medicamento)
        </dd>
        <dt class = "col-sm-2">
            Forma farmaceituca
        </dt>
        <dd class = "col-sm-10">
            @Html.DisplayFor(model => model.Forma_farmaceutica)
        </dd>
        <dt class = "col-sm-2">
            Efectos secundarios
        </dt>
        <dd class = "col-sm-10">
            @Html.DisplayFor(model => model.Efectos_secundarios)
        </dd>
    </dl>
</div>
<div>
    <a asp-action="Edit" asp-route-id="@Model.Id">Ediar</a> |
    <a asp-action="Index">Volver a la lista</a>
</div>
